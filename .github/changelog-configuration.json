{
  "categories": [
    {
      "title": "## 🎉 Features",
      "labels": [
        "feature",
        "enhancement"
      ]
    },
    {
      "title": "## 🐛 Fixes",
      "labels": [
        "fix",
        "bug"
      ]
    },
    {
      "title": "## 🛠 Breaking Changes",
      "labels": [
        "semver-major",
        "breaking-change"
      ]
    },
    {
      "key": "tests",
      "title": "## 🧪 Tests",
      "labels": [
        "test"
      ]
    },
    {
      "title": "## 🧪 Tests and some 🪄 Magic",
      "labels": [
        "test",
        "magic"
      ],
      "exclude_labels": [
        "no-magic"
      ],
      "exhaustive": true,
      "exhaustive_rules": "false",
      "empty_content": "- no matching PRs",
      "rules": [
        {
          "pattern": "open",
          "on_property": "status",
          "flags": "gu"
        }
      ]
    }
  ],
  "base_branches": [
    "main"
  ]
}
